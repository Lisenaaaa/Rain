{
	"name": "rain",
	"version": "0.0.0",
	"description": "",
	"main": "dist/index.js",
	"scripts": {
		"test": "yarn run lint && yarn run build",
		"build": "yarn rimraf dist/ && yarn tsc",
		"lint": "yarn eslint --ext js,jsx,ts,tsx src",
		"start": "node loading.js && yarn build && node --trace-warnings -r source-map-support/register dist/index.js"
	},
	"author": "Lisena",
	"license": "NONE",
	"private": true,
	"devDependencies": {
		"@types/lodash": "^4.14.175",
		"@types/module-alias": "^2.0.1",
		"@types/ms": "^0.7.31",
		"@types/node": "^16.10.2",
		"@types/pg": "^8.6.1",
		"@types/prettier": "^2.4.1",
		"@types/validator": "^13.6.3",
		"@typescript-eslint/eslint-plugin": "^4.32.0",
		"@typescript-eslint/parser": "^4.32.0",
		"discord-api-types": "^0.23.1",
		"eslint": "^7.32.0",
		"prettier": "^2.4.1",
		"rimraf": "^3.0.2",
		"source-map-support": "^0.5.19",
		"typescript": "^4.4.3"
	},
	"dependencies": {
		"chalk": "^4.1.2",
		"discord-akairo": "NotEnoughUpdates/discord-akairo",
		"discord.js": "NotEnoughUpdates/discord.js",
		"dotenv": "^10.0.0",
		"got": "^11.8.2",
		"module-alias": "^2.2.2",
		"ms": "^2.1.3",
		"nanoid": "^3.1.30",
		"pg": "^8.7.1",
		"pg-hstore": "^2.3.4",
		"sequelize": "^6.6.5"
	},
	"prettier": {
		"printWidth": 200,
		"tabWidth": 4,
		"useTabs": true,
		"semi": false,
		"singleQuote": true,
		"quoteProps": "as-needed",
		"trailingComma": "es5",
		"bracketSpacing": true,
		"arrowParens": "always",
		"embeddedLanguageFormatting": "auto"
	},
	"_moduleAliases": {
		"@functions": "dist/functions",
		"@extensions": "dist/extensions",
		"@src": "dist",
		"@types": "dist/types"
	},
	"eslintConfig": {
		"env": {
			"es2021": true,
			"node": true
		},
		"extends": [
			"eslint:recommended",
			"plugin:@typescript-eslint/recommended"
		],
		"parser": "@typescript-eslint/parser",
		"parserOptions": {
			"ecmaVersion": 12,
			"sourceType": "module"
		},
		"plugins": [
			"@typescript-eslint"
		],
		"ignorePatterns": [
			"dist"
		],
		"rules": {
			"@typescript-eslint/explicit-module-boundary-types": "off",
			"@typescript-eslint/no-var-requires": "off",
			"@typescript-eslint/ban-ts-comment": "off"
		}
	},
	"packageManager": "yarn@3.0.2"
}
