{
	"name": "rain-sapphire",
	"version": "0.0.1",
	"description": "Alyssa (lisenaaaa)'s Rain bot, but in Sapphire instead of discord-akairo!",
	"main": "dist/index.js",
	"scripts": {
		"test": "yarn run lint && yarn run build",
		"build": "yarn rimraf dist/ && yarn tsc",
		"lint": "yarn eslint --ext js,jsx,ts,tsx src",
		"start": "node starting.js && yarn build && node --trace-warnings --enable-source-maps dist/index.js"
	},
	"repository": "https://github.com/Lisenaaaa/Rain/tree/sapphire",
	"author": "Alyssa <69115669+Lisenaaaa@users.noreply.github.com>",
	"license": "UNLICENSED",
	"packageManager": "yarn@3.1.0",
	"devDependencies": {
		"@types/pg": "^8",
		"@types/rimraf": "^3",
		"@types/validator": "^13",
		"@typescript-eslint/eslint-plugin": "^5.3.1",
		"@typescript-eslint/parser": "^5.3.1",
		"discord-api-types": "^0.25.2",
		"eslint": "^8.2.0",
		"prettier": "^2.4.1",
		"rimraf": "^3.0.2",
		"tslib": "^2.3.1",
		"typescript": "^4.5.4",
		"validator": "^13.7.0"
	},
	"dependencies": {
		"@sapphire/decorators": "^4.0.2",
		"@sapphire/discord.js-utilities": "^4.4.0",
		"@sapphire/framework": "next",
		"@sapphire/plugin-editable-commands": "^1.1.3",
		"@sapphire/utilities": "^3.2.1",
		"@types/node": "^17.0.10",
		"chalk": "4",
		"discord.js": "^13.6.0",
		"got": "11.8.2",
		"moment": "^2.29.1",
		"ms": "^2.1.3",
		"nanoid": "^3.2.0",
		"pg": "^8.7.1",
		"pg-hstore": "^2.3.4",
		"sequelize": "^6.13.0"
	},
	"prettier": {
		"printWidth": 200,
		"tabWidth": 4,
		"useTabs": true,
		"semi": false,
		"singleQuote": true,
		"quoteProps": "as-needed",
		"trailingComma": "es5",
		"bracketSpacing": true,
		"arrowParens": "always",
		"embeddedLanguageFormatting": "auto"
	},
	"eslintConfig": {
		"env": {
			"es2021": true,
			"node": true
		},
		"extends": [
			"eslint:recommended",
			"plugin:@typescript-eslint/recommended"
		],
		"parser": "@typescript-eslint/parser",
		"parserOptions": {
			"ecmaVersion": 12,
			"sourceType": "module"
		},
		"plugins": [
			"@typescript-eslint"
		],
		"ignorePatterns": [
			"dist"
		],
		"rules": {
			"@typescript-eslint/explicit-module-boundary-types": "off",
			"@typescript-eslint/no-var-requires": "off",
			"@typescript-eslint/ban-ts-comment": "off"
		}
	}
}
